package com.vipul.DSA;

public class PrimeNumber {

	public static void main(String[] args) {
		
		int num = 6;
		int c=2;
		boolean flag = true;
		while(c*c<num)
		{
			if(num%c==0)
			{
				System.out.println("Not a prime number");
				flag = false;
				break;
			}
			c=c+1;
		}
		if(flag==true)
		{
			System.out.println("Prime number");
		} else
		{
			System.out.println("Not a Prime number");
		}

	}

}
